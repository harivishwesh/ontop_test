[PrefixDeclaration]
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
owl:		http://www.w3.org/2002/07/owl#
xsd:		http://www.w3.org/2001/XMLSchema#
obda:		https://w3id.org/obda/vocabulary#

[MappingDeclaration] @collection [[
mappingId	BOOTSTRAPPED-MAPPING-ID1
target		<http://semanticweb.org/clustering/a/id={id}> a <http://semanticweb.org/clustering/a/Male> . 
source		SELECT * FROM "a" WHERE ca = '1'

mappingId	BOOTSTRAPPED-MAPPING-ID2
target		<http://semanticweb.org/clustering/a/id={id}> a <http://semanticweb.org/clustering/a/Female> . 
source		SELECT * FROM "a" WHERE ca = '2'

mappingId	BOOTSTRAPPED-MAPPING-ID3
target		<http://semanticweb.org/clustering/a/id={id}> a <http://semanticweb.org/clustering/a> ; <http://semanticweb.org/clustering/a#id> {id}^^xsd:integer ; <http://semanticweb.org/clustering/a#ca> {ca}^^xsd:integer . 
source		SELECT * FROM "a"
]]

